{
    "MicroPython.executeButton": [
        {
            "text": "â–¶",
            "tooltip": "Run",
            "alignment": "left",
            "command": "extension.executeFile",
            "priority": 3.5
        }
    ],
    "MicroPython.syncButton": [
        {
            "text": "$(sync)",
            "tooltip": "sync",
            "alignment": "left",
            "command": "extension.execute",
            "priority": 4
        }
	],
	"files.associations": {
		"typeinfo": "c",
		"modhomekit.h": "c",
		"algorithm": "c",
		"chrono": "c",
		"limits": "c",
		"ratio": "c",
		"__config": "c",
		"memory": "c",
		"complex": "c"
	},
	"C_Cpp.dimInactiveRegions": false,
	"python.linting.pylintEnabled": true,
	"python.autoComplete.extraPaths": [
		"${workspaceFolder}/modules",
		"${workspaceFolder}/modules/lib",
		"${workspaceFolder}/modules/simul",
		"${workspaceFolder}/modules/lib/tools",
		"${workspaceFolder}/modules/lib/shell"
	],
	"python.analysis.extraPaths":[
		"${workspaceFolder}/modules",
		"${workspaceFolder}/modules/lib",
		"${workspaceFolder}/modules/simul",
		"${workspaceFolder}/modules/lib/tools",
		"${workspaceFolder}/modules/lib/shell"
	],
	"python.linting.pylintArgs": [
		"--disable=C",
		"--disable=R",
		"--disable=bad-indentation",
		"--disable=unused-argument",
		"--disable=invalid-name",
		"--disable=import-outside-toplevel",
		"--disable=broad-except",
		"--disable=unused-variable",
		"--disable=line-too-long",
		"--disable=superfluous-parens",
		"--disable=unidiomatic-typecheck",
		"--disable=bare-except",
		"--disable=unused-wildcard-import",
		"--disable=wildcard-import",
		"--disable=global-statement",
		"--disable=eval-used",
		"--disable=exec-used"],
	"python.envFile": "${workspaceFolder}/.env",
	"python.linting.pylintUseMinimalCheckers": true,
	"python.linting.enabled": true,
}