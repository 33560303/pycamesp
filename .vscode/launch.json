{
	"comment": "For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387",
	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "Pycameresp",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/modules/main.py",
			"cwd"    : "${workspaceFolder}/modules",
			"justMyCode": false
		},
		{
			"name": "Editor",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/modules/lib/shell/editor.py",
			"cwd"    : "${workspaceFolder}/modules",
		},
		{
			"name": "Shell",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/modules/lib/shell/shell.py",
			"cwd"    : "${workspaceFolder}/modules",
		},
		{
			"name": "Flasher",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tools/flasher/Main.py",
			"cwd": "${workspaceFolder}/tools/flasher",
		},
		{
			"name": "VT100",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tools/flasher/TermVT100.py",
			"cwd": "${workspaceFolder}/tools/flasher",
		}
	]
}